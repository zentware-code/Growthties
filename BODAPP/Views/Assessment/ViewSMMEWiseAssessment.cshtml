
@{
    ViewBag.Title = "ViewSMMEWiseAssessment";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}
@{

    var Enterpriseuser = Session["EnterpriseUserDataModel"] as BODDal.Models.UserModel;
    var user = Session["UserDataModel"] as BODDal.Models.UserModel;
    var SmmeUser = Session["SMMEUserDataModel"] as BODDal.Models.UserModel;
    var EnterpriseEmpuser = Session["EnterpriseEMPUserDataModel"] as BODDal.Models.UserModel;
    var AdminUser = Session["AdminUserDataModel"] as BODDal.Models.UserModel;

    var LinkJs = "";
    if (Enterpriseuser != null)
    {
        LinkJs = Enterpriseuser.ChildMenuJsLink;
    }
    else if (SmmeUser != null)
    {
        LinkJs = SmmeUser.ChildMenuJsLink;
    }
    else if (EnterpriseEmpuser != null)
    {
        LinkJs = EnterpriseEmpuser.ChildMenuJsLink;
    }
    else if (AdminUser != null)
    {
        LinkJs = AdminUser.ChildMenuJsLink;
    }
    else
    {
        LinkJs = user.ChildMenuJsLink;
    }
}

<style>
    .no-radius {
        border-radius: 0 !important;
    }
    .input-error {
        border: 2px solid red !important;
    }
      .radio-error {
        border: 2px solid red;
        padding: 10px;
        border-radius: 5px;
    }

</style>



<div class="container-xxl flex-grow-1 container-p-y" id="section-block">
    <h4 class="py-3 mb-2">
        <span class="text-muted fw-light">Assessment /</span> View All Assesment Responses
    </h4>
    <div class="col-12 mt-4">
        <div class="row">
            <div class="col-md-12 mb-4 mb-md-2" id="responses-div">
                <input type="hidden" id="hdnSmmeId" value="@ViewBag.SmmeId" />
                <input type="hidden" id="hdnSmmeName" />
                <input type="hidden" id="hdnEmail"  />
                <input type="hidden" id="hdnPhoneNumber" />
               
                <div class="accordion mt-3" id="accordionExample">

                </div>
            </div>
        </div>
        
        <!-- Buttons -->
        @*<div class="mb-3 mt-3 d-flex gap-2">
            <button type="button" class="btn btn-danger" id="btnNotVerified">Not Verified</button>
            <button type="button" class="btn btn-success" id="btnVerified">Verified</button>
        </div>*@
        <!-- Fixed Textarea -->
        <div class="card bg-white">
            <div class="card-body">
                <div class="mb-3 mt-2 d-flex gap-2">
                    <textarea class="form-control" id="txtSummary" placeholder="Enter summary here..."></textarea>
                </div>
                <div class="mb-3 mt-3 d-flex gap-2">
                    <button type="button" class="btn btn-danger" id="btnNotVerified">Not Verified</button>
                    <button type="button" class="btn btn-success" id="btnVerified">Verified</button>
                </div>
            </div>
        </div>
        
    </div>
</div>




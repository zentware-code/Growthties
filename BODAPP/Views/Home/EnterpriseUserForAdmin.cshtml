
@{
    ViewBag.Title = "EnterpriseUserForAdmin";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}

<link href="~/Content/assets/vendor/libs/sweetalert2/sweetalert2.css" rel="stylesheet" />
<div class="container-xxl flex-grow-1 container-p-y">


    <h4 class="py-3 mb-4">
        <span class="text-muted fw-light">Enterprise /</span> Add Enterprise User
    </h4>
    <div class="card">
        <div class="card-body">
            <form id="formEnterpriseUser"  onsubmit="return false"  class="row mt-2">

                    <div class="col-md-4 mb-3">
                       
                            <label for="txtUserName" class="form-label">User Name</label>
                            <input type="text" class="form-control" id="txtUserName" placeholder="Type Username here" name="txtUserName" />
                            <input type="hidden" id="Id" />
                        
                    </div>
                    <div class="col-md-4 mb-3">
                      
                            <label for="txtUserEmail" class="form-label">User Email</label>
                            <input type="text" class="form-control" id="txtUserEmail" placeholder="Type Email here" name="txtUserEmail" />

                     
                    </div>
                    <div class="col-md-4 mb-3">
                       
                        <label for="txtContact" class="form-label">Contact Number</label>
                        <div class="input-group">
                            <span class="input-group-text">SA (+27)</span>
                            <input type="text" class="form-control" id="txtContact" placeholder="Type Contact here" name="txtContact" oninput="this.value = this.value.replace(/[^0-9.]/g, '').replace(/(\..*)\./g, '$1');" />
                        </div>
                      
                    </div>
              
                <br />
              
                    <div class="col-md-4 mb-3">
                        <label for="ddlEnterprise" class="form-label">Select Enterprise Name</label>
                        <select id="ddlEnterprise" name="ddlEnterprise" class="select2 form-select ddlEnterprise" data-allow-clear="true"></select>
                    </div>
                    <div class="col-md-4 mb-3">
                        <label for="ddlRole" class="form-label">Select Role</label>
                        <select id="ddlRole" name="ddlRole" class="select2 form-select ddlRole" data-allow-clear="true"></select>
                    </div>
              
               
                <div class="col-md-4 mb-3">

                    <label for="txtContact" class="form-label">Age</label>
                    <input type="text" class="form-control" id="txtAge" placeholder="Enter Age" name="txtAge" onkeypress="return isNumberKey(event)" />


                </div>




                <div class="col-md-4 mb-3">
                    <label for="ddlGender" class="form-label">Select Gender</label>
                    <select id="ddlGender" name="ddlGender" class="select2 form-select ddlGender" data-allow-clear="true">
                        <option value="">Select</option>
                        <option value="Male">Male</option>
                        <option value="Female">Female</option>
                    </select>
                </div>
                    <div class="col-md-12 mt-2">
                        <div class="demo-inline-spacing">

                            @*<button type="submit" class="btn btn-primary  btn-shadow-primary" id="btnUpdateNext">
                                <span class="ti-xs ti ti-plus me-1"></span>Submit
                            </button>*@

                            <button type="submit" class="btn btn-primary btn-shadow-primary" id="btnUpdateNext">
                                <span class="ti-xs ti ti-plus me-1"></span>Submit
                            </button>
                            <a class="btn btn-label-secondary waves-effect" id="btnCancelRedirect">
                                <span class="fa-solid fa-xmark me-1"></span>Cancel
                                @*ViewAllEnterpriseUserForAdmin*@
                            </a>
                        </div>
                    </div>
               

            </form>
        </div>
</div>


</div>
<script type="text/javascript">
    var URLList = {

        SaveRecordData: '@Url.Action("InsertEnterpriseUserAdmin", "ScriptJson")',
        GetEntityMasterById: '@Url.Action("GetEnterpriseUserAdmin", "ScriptJson")'
    };
    var globalData = {
        TransactionType: 'Select',
        Param: 'UM_Id',
        //paramValue: parseInt(id),
        StoreProcedure: 'UserMaster_USP'
    };
    
</script>

<!-- Vendors JS -->
<script src="~/Content/assets/vendor/libs/%40form-validation/umd/bundle/popular.min.js"></script>
<script src="~/Content/assets/vendor/libs/%40form-validation/umd/plugin-bootstrap5/index.min.js"></script>
<script src="~/Content/assets/vendor/libs/%40form-validation/umd/plugin-auto-focus/index.min.js"></script>
<script src="~/Content/assets/vendor/libs/select2/select2.js"></script>
<script src="~/Content/assets/vendor/libs/quill/katex.js"></script>
<script src="~/Content/assets/vendor/libs/quill/quill.js"></script>
<script src="~/Content/assets/vendor/libs/bootstrap-select/bootstrap-select.js"></script>
<script src="~/Content/assets/vendor/libs/sweetalert2/sweetalert2.js"></script>

<div class="container-xxl flex-grow-1 container-p-y">



    <h4 class="py-3 mb-2">
        <span class="text-muted fw-light">Admin /</span> Item Setup
    </h4>

    <div class="app-ecommerce-category">
        <!-- Category List Table -->
        <div class="card">
            <div class="card-datatable table-responsive">
                <table class="datatables-category-list table border-top" id="datatable-example">
                    <thead>
                        <tr>
                          
                            <th class="text-nowrap">Item Name</th>
                            <th class="text-nowrap">Description</th>
                            <th class="text-lg-center">Actions</th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
        <!-- Offcanvas to add new sector -->
        <div class="offcanvas offcanvas-end" tabindex="-1" id="setUPFormPopUp" aria-labelledby="offcanvassetUPLabel">
            <!-- Offcanvas Header -->
            <div class="offcanvas-header py-4">
                <h5 id="setUPFormPopUpLabel" class="offcanvas-title">Add Sector</h5>
                <button type="button" class="btn-close bg-label-secondary text-reset" data-bs-dismiss="offcanvas" aria-label="Close"></button>
            </div>
            <!-- Offcanvas Body -->
            <div class="offcanvas-body border-top">
                <form class="pt-0" id="setUPForm" novalidate="novalidate">
                    <!-- Title -->
                    <div class="mb-3">
                        <label class="form-label" for="sector-name">Item Name</label>
                        <input type="text" class="form-control" id="ItemName" placeholder="Enter ItemName" name="ItemName" aria-label="sector-name" >
                        <input type="hidden" id="Id" name="Id" class="form-control"/>
                    </div>
                    <!-- Slug -->
                    <div class="mb-3">
                        <label class="form-label" for="description">Description</label>
                        <textarea type="text" id="ItemDescription" class="form-control" placeholder="Enter Description" aria-label="description" name="ItemDescription"></textarea>
                    </div>
                    <!-- Submit and reset -->
                    <div class="mb-3 position-absolute bottom-2">
                     
                        <button id="btnSave" type="submit" class="btn btn-primary me-sm-3 me-1 data-submit btn-shadow-primary">Submit</button>
                        <button type="reset" class="btn btn-label-secondary waves-effect close" data-bs-dismiss="offcanvas">Discard</button>
                    </div>
                </form>
            </div>
        </div>
    </div>

</div>
<script type="text/javascript">
    var URLList = {

        SaveRecord: '@Url.Action("ItemSetUp", "ScriptJson")',
        GetList: '@Url.Action("ItemSetUpList", "ScriptJson")',
        GetEntityMasterById: '@Url.Action("GetGlobalMaster", "ScriptJson")'
    };
    var globalData = {
        TransactionType: 'Select',
        Param: 'IM_Id',
        //paramValue: parseInt(id),
        StoreProcedure: 'ItemSetUp_USP'
    };
    var search = 'Search Item';
    var popup = "#setUPFormPopUp";
    var dtTable = 'item';
    var columnData = [{ data: "ItemName" },
                   { data: "ItemDescription" }]
</script>

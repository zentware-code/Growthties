
@{
    ViewBag.Title = "TeamBuildingList";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}

<!-- Core CSS -->

<link rel="stylesheet" href="~/Content/assets/vendor/libs/datatables-bs5/datatables.bootstrap5.css">
<link rel="stylesheet" href="~/Content/assets/vendor/libs/datatables-responsive-bs5/responsive.bootstrap5.css">
<link rel="stylesheet" href="~/Content/assets/vendor/libs/datatables-buttons-bs5/buttons.bootstrap5.css">

<link href="~/Content/assets/vendor/libs/sweetalert2/sweetalert2.css" rel="stylesheet" />
<div class="container-xxl flex-grow-1 container-p-y">



    <h4 class="py-3 mb-2">
        <span class="text-muted fw-light">SMME /</span> All Team 
    </h4>

    <div class="app-ecommerce-category">
        <!-- Category List Table -->
        <div class="card">
            <div class="card-datatable table-responsive">
                <input type="hidden" id="hdnTeamSmmeId" value="@ViewBag.SmmeId"/>
                <table class="datatables-category-list table border-top" id="datatable-example">
                    <thead>
                        <tr>

                            <th class="text-nowrap">Team Name</th>
                            <th class="text-nowrap">Description</th>
                            <th class="text-lg-center">Actions</th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
     
    </div>
    <div class="offcanvas offcanvas-end" tabindex="-1" id="setUPFormPopUp" aria-labelledby="offcanvassetUPLabel">
        <!-- Offcanvas Header -->
        <div class="offcanvas-header py-4">
            <h5 id="setUPFormPopUpLabel" class="offcanvas-title">View User</h5>
            <button type="button" class="btn-close bg-label-secondary text-reset" data-bs-dismiss="offcanvas" aria-label="Close"></button>
        </div>

     
   
        <!-- Offcanvas Body -->
        <div class="offcanvas-body border-top">
            <span class="float-end" style="display: block;">Make as Team Leader</span>
            <ul class="p-0" id="ulUserList" style="margin-top: 27px;"></ul>
        </div>
    </div>
</div>
<script type="text/javascript">
    var URLList = {

        GetList: '@Url.Action("GetGlobalMasterTransaction", "ScriptJson")',

    };


    var globalData = {
        TransactionType: 'SelectList',
        Param: 'BT_SMMEId',
        paramValue: $('#hdnTeamSmmeId').val(),
        StoreProcedure: 'BuildTeam_USP'
    };
    var search = 'Search Team';
    //var popup = "#setUPFormPopUp";
    var columnData = [{ data: "BT_TeamName" },
                   { data: "BT_Description" }]
</script>
<!-- endbuild -->
<!-- Vendors JS -->

<script src="~/Content/assets/vendor/libs/datatables-bs5/datatables-bootstrap5.js"></script>


<script src="~/Content/assets/vendor/libs/sweetalert2/sweetalert2.js"></script>

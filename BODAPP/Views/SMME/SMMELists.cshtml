
@{
    ViewBag.Title = "SMMELists";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}



<link rel="stylesheet" href="~/Content/assets/vendor/libs/datatables-bs5/datatables.bootstrap5.css">
<link rel="stylesheet" href="~/Content/assets/vendor/libs/datatables-responsive-bs5/responsive.bootstrap5.css">
<link rel="stylesheet" href="~/Content/assets/vendor/libs/datatables-buttons-bs5/buttons.bootstrap5.css">

<div class="container-xxl flex-grow-1 container-p-y">
    <h4 class="py-3 mb-2">
        <span class="text-muted fw-light">MSME /</span> View All MSMEs
    </h4>
    <div class="row g-4 mb-4">
        <div class="col-sm-6 col-xl-3">
            <a href="@Url.Action("SMMELists", "SMME")" onclick="getValue(30)">
                <div class="card">
                    <div class="card-body">
                        <div class="d-flex align-items-start justify-content-between">
                            <div class="content-left">
                                <p>All MSMEs on the System</p>
                                <div class="d-flex align-items-center my-2">
                                    <h3 class="mb-0 me-2"><span id="spnTotSmme"></span></h3>
                                    @*<p class="text-success mb-0">(+29%)</p>*@
                                </div>
                                <!-- <p class="mb-0">Last week analytics </p> -->
                            </div>
                            <div class="avatar">
                                <span class="avatar-initial rounded bg-label-primary">
                                    <i class="ti ti-user ti-sm"></i>
                                </span>
                            </div>
                        </div>
                    </div>
                </div>
                </a>
</div>

        <div class="col-sm-6 col-xl-3">
            <a href="@Url.Action("SMMELists", "SMME", new { Filter = "Active" })" onclick="getValue(30);">
                <div class="card">
                    <div class="card-body">
                        <div class="d-flex align-items-start justify-content-between">
                            <div class="content-left">
                                <p>All Verified MSMEs</p>
                                <div class="d-flex align-items-center my-2">
                                    <h3 class="mb-0 me-2"><span id="spnActiveSmme"></span></h3>
                                    @*<p class="text-danger mb-0">(-14%)</p>*@
                                </div>
                                <!-- <p class="mb-0">Last week analytics</p> -->
                            </div>
                            <div class="avatar">
                                <span class="avatar-initial rounded bg-label-success">
                                    <i class="ti ti-user-check ti-sm"></i>
                                </span>
                            </div>
                        </div>
                    </div>
                </div>
                </a>
</div>
        <div class="col-sm-6 col-xl-3">
            <a href="@Url.Action("SMMELists", "SMME", new { Filter = "Deactive" })" onclick="getValue(30);">
                <div class="card">
                    <div class="card-body">
                        <div class="d-flex align-items-start justify-content-between">
                            <div class="content-left">
                                <p>All Non-Vetted MSMEs</p>
                                <div class="d-flex align-items-center my-2">
                                    <h3 class="mb-0 me-2"><span id="spnDeactiveSmme"></span></h3>
                                    @*<p class="text-success mb-0">(+18%)</p>*@
                                </div>
                                <!-- <p class="mb-0">Last week analytics </p> -->
                            </div>
                            <div class="avatar">
                                <span class="avatar-initial rounded bg-label-danger">
                                    <i class="ti ti-user-plus ti-sm"></i>
                                </span>
                            </div>
                        </div>
                    </div>
                </div>
                </a>
</div>
        <div class="col-sm-6 col-xl-3">
            <a href="@Url.Action("SMMELists", "SMME", new { Filter = "Pending" })" onclick="getValue(30);">
                <div class="card">
                    <div class="card-body">
                        <div class="d-flex align-items-start justify-content-between">
                            <div class="content-left">
                                <p>All Supported MSMEs</p>
                                <div class="d-flex align-items-center my-2">
                                    <h3 class="mb-0 me-2"><span id="spnPendSmme"></span></h3>
                                    @*<p class="text-success mb-0">(+42%)</p>*@
                                </div>
                                <!-- <p class="mb-0">Last week analytics</p> -->
                            </div>
                            <div class="avatar">
                                <span class="avatar-initial rounded bg-label-warning">
                                    <i class="ti ti-user-exclamation ti-sm"></i>
                                </span>
                            </div>
                        </div>
                    </div>
                </div>
                </a>
</div>
    </div>


    <!-- Users List Table -->
    <div class="card">
        <div class="card-header border-bottom d-none">
            <h5 class="card-title mb-3">Search Filter</h5>
            <div class="d-flex justify-content-between align-items-center row pb-2 gap-3 gap-md-0">
                <div class="col-md-4 user_role"></div>
                <div class="col-md-4 user_plan"></div>
                <div class="col-md-4 user_status"></div>
            </div>
        </div>
        <div class="card-datatable table-responsive">
            <table class="datatables-users table" id="datatable-example">
                <thead class="border-top">
                    <tr>
                        <th></th>
                        <th>MSME NAME </th>
                        <th>INDUSTRY</th>
                        <th>CONTACT NUMBER</th>
                        <th>REGISTRATION NUMBER</th>
                        <th>REGISTRATION DATE</th>
                        <th>Status</th>
                        <th>Actions</th>
                    </tr>
                </thead>


            </table>
        </div>
        <!-- Offcanvas to add new user -->

    </div>
</div>


<script type="text/javascript">
    var URLList = {

        GetList: '@Url.Action("GetGlobalMasterTransaction", "ScriptJson")',

    };

</script>
<script src="~/Content/assets/vendor/libs/moment/moment.js"></script>
<script src="~/Content/assets/vendor/libs/datatables-bs5/datatables-bootstrap5.js"></script>
<script src="~/Content/assets/vendor/libs/select2/select2.js"></script>

<script src="~/Content/assets/vendor/libs/cleavejs/cleave.js"></script>
<script src="~/Content/assets/vendor/libs/cleavejs/cleave-phone.js"></script>


<script src="~/Scripts/custom/SMMEList.js"></script>



@{
    ViewBag.Title = "ViewAllBranch";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
    @Html.Partial("~/Views/Shared/_ProjectWiseSMMEModal.cshtml");
}

<link rel="stylesheet" href="~/Content/assets/vendor/libs/datatables-bs5/datatables.bootstrap5.css">
<link rel="stylesheet" href="~/Content/assets/vendor/libs/datatables-responsive-bs5/responsive.bootstrap5.css">
<link rel="stylesheet" href="~/Content/assets/vendor/libs/datatables-buttons-bs5/buttons.bootstrap5.css">
<link href="~/Content/assets/vendor/libs/sweetalert2/sweetalert2.css" rel="stylesheet" />
<div class="container-xxl flex-grow-1 container-p-y" id="section-block">
    <h4 class="py-3 mb-4">
        <span class="text-muted fw-light">Enterprise /</span> All Branch
    </h4>
    <!-- Users List Table -->
    <div class="card">
        <div class="card-header border-bottom d-none">
            <h5 class="card-title mb-3">Search Filter</h5>
            <div class="d-flex justify-content-between align-items-center row pb-2 gap-3 gap-md-0">
                <div class="col-md-4 user_role"></div>
                <div class="col-md-4 user_plan"></div>
                <div class="col-md-4 user_status"></div>
            </div>
        </div>
        <div class="card-datatable table-responsive">
            <input type="hidden" id="EntrId" name="EntrId" value="@ViewBag.EntrId" />
            <table class="datatables-users table" id="datatable-example">
                <thead class="border-top">
                    <tr>
                        <th></th>
                        <th>Branch Name</th>
                        <th>Email</th>
                        <th>Contact No</th>
                        <th>City</th>
                        <th>MSME</th>
                        <th>Actions</th>
                    </tr>
                </thead>


            </table>
        </div>
        <!-- Offcanvas to add new user -->

    </div>

    <div class="offcanvas offcanvas-end" tabindex="-1" id="setUPFormPopUp" aria-labelledby="offcanvassetUPLabel">
        <!-- Offcanvas Header -->
        <div class="offcanvas-header py-4">
            <h5 id="setUPFormPopUpLabel" class="offcanvas-title">Tag User To Branch</h5>
            <button type="button" class="btn-close bg-label-secondary text-reset" data-bs-dismiss="offcanvas" aria-label="Close"></button>
        </div>

      
        
        <!-- Offcanvas Body -->
        <div class="offcanvas-body border-top">
            <ul class="p-0 m-0" id="ulUserList"></ul>
        </div>
    </div>
    <div class="modal fade" id="shareProject" tabindex="-1" aria-hidden="true">
        <div class="modal-dialog modal-lg modal-dialog-centered modal-dialog-scrollable">
            <div class="modal-content p-3 p-md-5">
                <div class="modal-body">
                    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                    <div class="text-center">
                        <h3 class="mb-2">All MSME</h3>
                        <p>Share Branch with MSME</p>
                    </div>

                    <h4 class="mb-4 pb-2">MSME Details</h4>

                    <ul class="list-unstyled" id="ulSMMEList">
                        <!-- Dynamically inserted list items go here -->
                    </ul>
                </div>
            </div>
        </div>
    </div>

    <div class="offcanvas offcanvas-end" tabindex="-1" id="setProject" aria-labelledby="offcanvassetUPLabel">
        <!-- Offcanvas Header -->
        <div class="offcanvas-header py-4">
            <h5 id="setUPFormPopUpLabel" class="offcanvas-title">Tag Project To Branch</h5>
            <button type="button" class="btn-close bg-label-secondary text-reset" data-bs-dismiss="offcanvas" aria-label="Close"></button>
        </div>



        <!-- Offcanvas Body -->
        <div class="offcanvas-body border-top">
            <ul class="p-0 m-0" id="ulProjectList"></ul>
        </div>
    </div>
</div>
<script type="text/javascript">
    var URLList = {
        GetList: '@Url.Action("GetGlobalMasterTransaction", "ScriptJson")',
    };
  
</script>
<script src="~/Content/assets/vendor/libs/moment/moment.js"></script>
<script src="~/Content/assets/vendor/libs/datatables-bs5/datatables-bootstrap5.js"></script>
<script src="~/Content/assets/vendor/libs/select2/select2.js"></script>

<script src="~/Content/assets/vendor/libs/cleavejs/cleave.js"></script>
<script src="~/Content/assets/vendor/libs/cleavejs/cleave-phone.js"></script>

<script src="~/Content/assets/vendor/libs/sweetalert2/sweetalert2.js"></script>
<script src="~/Scripts/custom/ViewAllBranch.js"></script>
<script src="~/Scripts/utility/BranchWiseSMMEModal.js"></script>



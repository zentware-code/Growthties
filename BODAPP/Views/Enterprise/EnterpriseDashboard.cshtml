<link href="~/Content/assets/vendor/libs/sweetalert2/sweetalert2.css" rel="stylesheet" />
@{

    var Enterpriseuser = Session["EnterpriseUserDataModel"] as BODDal.Models.UserModel;
    var user = Session["UserDataModel"] as BODDal.Models.UserModel;
    var SmmeUser = Session["SMMEUserDataModel"] as BODDal.Models.UserModel;
    var EnterpriseEmpuser = Session["EnterpriseEMPUserDataModel"] as BODDal.Models.UserModel;
    var AdminUser = Session["AdminUserDataModel"] as BODDal.Models.UserModel;
    var userName = "";
    var profilePic = "";
    var prefix = "";
    var subrole = "";
    var role = "";
    var CompanyPic = "";
    var ThemeStyle = "";
    var ThemeColor = "";
    var TheamLink = "";
    var CoreLink = "";
    var CustomCSSLink = "";
    var Title = "";

    if (Enterpriseuser != null)
    {
        userName = Enterpriseuser.UserName;
        profilePic = Enterpriseuser.UM_ProfilePic;
        prefix = Enterpriseuser.UM_Prefix;
        subrole = Enterpriseuser.UM_SubRole;
        role = Enterpriseuser.UM_Role;
        CompanyPic = Enterpriseuser.UM_CompanyPic;
        ThemeStyle = Enterpriseuser.ThemeStyle;
        TheamLink = Enterpriseuser.TheamLink;
        CoreLink = Enterpriseuser.CoreLink;
        CustomCSSLink = Enterpriseuser.CustomCSSLink;
        Title = "ESD Platform - " + Enterpriseuser.UserName;
        ThemeColor = Enterpriseuser.ThemeColor;


    }

    else if (SmmeUser != null)
    {
        userName = SmmeUser.UserName;
        profilePic = SmmeUser.UM_ProfilePic;
        prefix = SmmeUser.UM_CmnyPrefix;
        subrole = SmmeUser.UM_SubRole;
        role = SmmeUser.UM_Role;
        CompanyPic = SmmeUser.UM_CompanyPic;
        ThemeStyle = SmmeUser.ThemeStyle;
        TheamLink = SmmeUser.TheamLink;
        CoreLink = SmmeUser.CoreLink;
        CustomCSSLink = SmmeUser.CustomCSSLink;
        Title = "ESD Platform - " + SmmeUser.UserName;
        ThemeColor = SmmeUser.ThemeColor;

    }
    else if (EnterpriseEmpuser != null)
    {
        userName = EnterpriseEmpuser.UserName;
        profilePic = EnterpriseEmpuser.UM_ProfilePic;
        prefix = EnterpriseEmpuser.UM_Prefix;
        subrole = EnterpriseEmpuser.UM_SubRole;
        role = EnterpriseEmpuser.UM_Role;
        CompanyPic = EnterpriseEmpuser.UM_CompanyPic;
        ThemeStyle = EnterpriseEmpuser.ThemeStyle;
        TheamLink = EnterpriseEmpuser.TheamLink;
        CoreLink = EnterpriseEmpuser.CoreLink;
        CustomCSSLink = EnterpriseEmpuser.CustomCSSLink;
        Title = "ESD Platform - " + EnterpriseEmpuser.UserName;
        ThemeColor = EnterpriseEmpuser.ThemeColor;
    }
    else if (AdminUser != null)
    {
        userName = AdminUser.UserName;
        profilePic = AdminUser.UM_ProfilePic;
        prefix = AdminUser.UM_Prefix;
        subrole = AdminUser.UM_SubRole;
        role = AdminUser.UM_Role;
        CompanyPic = AdminUser.UM_CompanyPic;
        ThemeStyle = AdminUser.ThemeStyle;
        TheamLink = AdminUser.TheamLink;
        CoreLink = AdminUser.CoreLink;
        CustomCSSLink = AdminUser.CustomCSSLink;
        Title = "ESD Platform - Admin" + AdminUser.UserName;
        ThemeColor = AdminUser.ThemeColor;
    }
    else
    {
        userName = user.UserName;
        profilePic = user.UM_ProfilePic;
        prefix = user.UM_Prefix;
        subrole = user.UM_SubRole;
        role = user.UM_Role;
        ThemeStyle = user.ThemeStyle;
        TheamLink = user.TheamLink;
        CoreLink = user.CoreLink;
        CustomCSSLink = user.CustomCSSLink;
        Title = "ESD Platform - Admin";
        ThemeColor = user.ThemeColor;
    }
}


                <li class="nav-item"><a class="nav-link curser-point" href="javascript:void(0);" onclick="getValue(63)"><i class='ti-xs ti ti-layout-dashboard me-1'></i> Dashboard</a></li>

            </ul>
        </div>
    </div>
    <!--/ Navbar pills -->


    <div class="row g-4" id="ulUserList">

    </div>


    <div class="offcanvas offcanvas-end" tabindex="-1" id="setUPFormPopUp" aria-labelledby="offcanvassetUPLabel">
        <!-- Offcanvas Header -->
        <div class="offcanvas-header py-4">
            <h5 id="setUPFormPopUpLabel" class="offcanvas-title">View Users</h5>
            <button type="button" class="btn-close bg-label-secondary text-reset" data-bs-dismiss="offcanvas" aria-label="Close"></button>
        </div>

        <!-- Offcanvas Body -->
        <div class="offcanvas-body border-top">
            <ul class="p-0 m-0" id="ulUserList"></ul>
        </div>
    </div>

    <div class="offcanvas offcanvas-end" tabindex="-1" id="setUPFormPopUpMSME" aria-labelledby="offcanvassetUPLabel1">

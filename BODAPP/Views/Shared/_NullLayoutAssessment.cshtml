<style>
    #theme-setting-btn {
    position: absolute;
    top: 180px;
    right: -60px;
    z-index: 9;
    display: block;
    width: 42px;
    height: 42px;
    border-top-left-radius: 15%;
    border-bottom-left-radius: 15%;
    background-color: var(--default-primary-hex)!important;
    color: #fff !important;
    text-align: center;
    font-size: 18px !important;
    line-height: 42px;
    opacity: 1;
    -webkit-transition: all .1slinear .2s;
    -o-transition: all .1s linear .2s;
    transition: all .1slinear .2s;
    -webkit-transform: translateX(-62px);
    -ms-transform: translateX(-62px);
    transform: translateX(-62px);
}
.theme-customizer-inner {
    position: relative;
    overflow: auto;
    flex: 0 1 auto;
    -webkit-box-flex: 0;
    opacity: 1;
    transition: opacity .2s;
}

.theme-customizer-colors-option {
    display: flex;
    flex-direction: row;
    justify-content: space-around;
    margin: 0;
    gap: .3rem;
}

    .theme-customizer-colors-option .custom-option {
        inline-size: 50px;
        padding:0;

    }

    .theme-customizer-colors-option .custom-option-body {
        border-radius: .5rem;
        block-size: 30px;
        inline-size: 30px;
    }
    .theme-customizer-colors-option .custom-option .custom-option-content {
    padding: 0;
    min-block-size: 46px;
    display:flex;
    justify-content:center;
    align-items:center;
}

   .custom-option-body img{
    display: block;
    margin: auto;
    border: 1px solid;
    border-color:#dfdfdf;
    border-radius: 8px;
    }
   .custom-option-body img.active{

    border: 2px solid;
    border-color: var(--default-primary-hex);
    
    }
   .form-check.custom-option.custom-option-icon-mode.mb-0 {
    border: none;
    width:108px
}
     :root {
    --default-primary-rgb:@ThemeColor;
    --default-primary-hex:@CustomCSSLink;
}
</style>
